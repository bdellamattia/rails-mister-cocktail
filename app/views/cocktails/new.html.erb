<div class="container">
<h1>Create your own<!-- <span id="banner-typed-text-2"> --></span></h1>
<div class="row">
<div class="col-xs-12 col-sm-4">

<%= simple_form_for (@cocktail) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Create this amazing cocktail", class: "btn btn-primary" %>
<% end %>
</div>
</div>
