<div class="container">
<h1>it's time to create your own<!-- <span id="banner-typed-text-2"> --></span></h1>
<div class="row">
<div class="col-xs-12 col-sm-4">

<%= simple_form_for (@cocktail) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "create this amazing cocktail", class: "btn btn-primary" %>
<% end %>
<br><br>
<div><%= link_to "baby come back", cocktails_path, class: "back" %></div>
</div>
</div>
